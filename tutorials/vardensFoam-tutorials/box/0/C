FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      C;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0;

boundaryField
{
     x0
    {
        type            fixedValue;
	value           uniform 1.0;
    }
    x1
    {
        type            zeroGradient;
    }
    y0
    {
        type            zeroGradient;
    }
    y1
    {
        type            zeroGradient;
    }
    z1
    {
        type            empty;
    }
    z0
    {
        type            empty;
    }
}


