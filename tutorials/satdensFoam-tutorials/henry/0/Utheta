FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "0";
    object      Utheta;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{

    x0
    {
      //type            zeroGradient;
      type            fixedValue;
      value           uniform (6.6e-5 0.0 0.0);
    }

    x1
    {
        type            zeroGradient;
    }

    y0
    {
        type            zeroGradient;
    }

    y1
    {
        type            zeroGradient;
    }

    z0
    {
        type            empty;
    }

    z1
    {
        type            empty;
    }

}


