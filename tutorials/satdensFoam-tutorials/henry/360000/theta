FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "360000";
    object      theta;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 1;

boundaryField
{
    x0
    {
        type            calculated;
        value           uniform 1;
    }
    x1
    {
        type            calculated;
        value           uniform 1;
    }
    y0
    {
        type            calculated;
        value           uniform 1;
    }
    y1
    {
        type            calculated;
        value           uniform 1;
    }
    z0
    {
        type            empty;
    }
    z1
    {
        type            empty;
    }
}


