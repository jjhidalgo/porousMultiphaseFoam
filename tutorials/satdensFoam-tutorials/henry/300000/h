            const vector axis(0, 1, 0);
	    operator==(1.02499 - 0.02499*mag(this->patch().Cf() & axis));

        #};
        codeOptions     #{
          -I$(FOAM_CASE)/incl                 \
          -I$(LIB_SRC)/finiteVolume/lnInclude \
          -I$(LIB_SRC)/meshTools/lnInclude
        #};
